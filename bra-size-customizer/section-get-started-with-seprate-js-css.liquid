    </section>
    
    <section class="section question" id="section-4" style="display: none;">
    <!-- Section 4 -->
     <div class="container-set">
      <div class="content-center">
        <span class="second-title">Step 4</span>
        <span class="section-title">Add some fun straps.</span>
        <p class="bold">one, two, pick a few.</p>
      </div>
      <div class="fields-set">
        {% paginate collections['straps'].products by 1000 %}
        {% for product in collections['straps'].products %}
        <div class="one-third column straps-size-container{%comment%}{% for tag in product.tags %}{% if tag != 'Black' %} disabled{% endif %}{% endfor %}{%endcomment%}" data-color="{% for tag in product.tags %}{% if tag == 'Black' or tag == 'Beige' %}{{tag}}{% endif %}{% endfor %}">
          <a href="#" class=""><img class="cup-size" src="{{product.featured_image | img_url: '700x700'}}" alt="{{product.title | escape}}"></a>
          <div class="overlay">
            <div class="text">
              <span class="product-title">{{product.title}}</span>
              <span class="product-price">{{product.price | money}}</span>
              <select name="disabled" class="variant-straps-size" id="variant-{% increment variantcount %}"{% if product.options.size < 2 %} style="display:none;"{% endif %}>{% for variant in product.variants %}
                <option{% if variant == product.selected_or_first_available_variant %} selected="selected"{% endif %} data-stock="{% if variant.available %}true{% else %}false{% endif %}" value="{{ variant.id }}">{{ variant.title }}{% if variant.available %}{% else %} - {{ 'products.product.sold_out' | t }}{% endif %}</option>{% endfor %}
              </select>
            </div>
          </div>
        </div>
        {% endfor %}
        {% endpaginate %}
      </div>
     
      <p></p>
      <a href="#" class="btn button" id="setp-addtocart">Add to cart</a>
      <div class="fields-set">
        <div class="sixteen columns alpha">
          <p class="content-space inline-block"></p>
        </div>
      </div>
     </div>
    </section>
    
    <section class="section question" id="section-5" style="display: none;">
     <div class="container-set">
      <div class="mbottom space"></div>
      <div class="middle-content">
      <p class="main-title">
          <span class="pink">h</span>
          <span class="blue">0</span>
          <span class="green">0</span>
          <span class="red">r</span>
          <span class="pink">a</span>
          <span class="blue">y</span>
          <span class="green">!</span>
      </p>
      <p class="bold top">Your new favorite bra is ready to meet you.</p>
      <div class="content-center">
        <div class="progress"></div>
        <div class="section-space"></div>
        <div class="checkout-feilds">
          <a href="/collections/all" class="btn button next-section cont">Keep Shopping</a>
        </div>
        <div class="checkout-feilds">
          <a href="/checkout" id="checkoutbutton" class="btn button disabled">Checkout</a>
        </div>
       </div>
      </div>
     </div>
    </section>
    </div>
  </div>
</div>
