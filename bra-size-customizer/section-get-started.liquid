<script type="text/javascript">
  
 $(document).ready(function() {
   
   $(document).on('click', '.next-section', function(e) {
     $('.section').removeClass('activate').hide();
     if($(this).parents('.section').next('.section').length > 0) {
       $(this).parents('.section').next('.section').addClass('activate').show();
       $('.coffee-club-form #question1').each(function(){
         $('.question a[href*="#"]').click(function(e){

         });
       });
     } return false;
   });
   $(document).on('click', '#newsletter-link', function(e) {
     $('#section-newsletter').show();
   });
   $(document).on('click', '.newsletter-close', function(e) {
     $('#section-newsletter').hide();
   });
   $('#bra-size').on('change', function(){
     var valcupsize = $(this).find(':selected').val();
     var cupsize = $('#cup-size');
     $('#setp-1').addClass('disable');
     $('#cup-size').val('0');
     $('option', cupsize).filter(function(){
        if ($(this).attr('data-group') === valcupsize || $(this).attr('data-group') === 'Show') { if ($(this).parent('span').length) { $(this).unwrap(); } } else { if (!$(this).parent('span').length) { $(this).wrap( "<span>" ).parent().hide(); } }
     });
   });
   $('#bra-size').trigger('change');
   //$('body').on('change','#cup-size', function() {
   $('#cup-size').on('change', function(){
     $('#setp-1').removeClass('disable');
     $('#cup-size-value').val($('#cup-size').val()).attr('value', $('#cup-size').val());
     var valvariantcupsize = $(this).find(':selected').attr('data-cupsize');
     var variantcupsize = $('.variant-cup-size');
     $('option', variantcupsize).filter(function(){
        if ($(this).attr('data-cupsize') === valvariantcupsize) { $(this).attr("disabled", false).attr('selected', true); } else { $(this).attr('selected', false).attr("disabled", true); }
        $('.variant-cup-size').each(function(index, element) {
          var addClass1 = true, options = element.options;
          for (i = 0; i < options.length; i++) {
            var selected1 = options[i].disabled
            if(selected1 == false){ addClass1 = false; break; }
          }
          var stock1 = $(this).find(':selected').trigger('change').attr('data-stock');
          if ((stock1 === 'false') || (addClass1)) {
            $(this).parents('.cup-size-container').addClass('disabled outofstock').find('.stock').show();
          }
        });
     });
     
     var valvariantbackssize = $(this).find(':selected').attr('data-backssize');
     var variantbackssize = $('.variant-backs-size');
     $('option', variantbackssize).filter(function(){
        if ($(this).attr('data-backssize') === valvariantbackssize) { $(this).attr("disabled", false).attr('selected', true); } else { $(this).attr('selected', false).attr("disabled", true); }
        $('.variant-backs-size').each(function(index, element) {
          var addClass2 = true, options = element.options;
          for (i = 0; i < options.length; i++) {
            var selected2 = options[i].disabled
            if(selected2 == false){ addClass2 = false; break; }
          }
          var stock2 = $(this).find(':selected').trigger('change').attr('data-stock');
          if ((stock2 === 'false') || (addClass2)) {
            $(this).parents('.backs-size-container').addClass('disabled outofstock').find('.stock').show();
          }
        });
     });
   });
   $(document).on('click', '.cup-size-container:not(.disabled)', function(e) {
     $('.cup-size-container:not(.disabled)').removeClass('selected-outline').find('select').attr('name', 'disabled');
     $(this).addClass('selected-outline').find('select').attr('name', 'id');
     return false;
   });
   $('.cup-size-container:not(.outofstock)').addClass('disabled');
   $('.cup-size-container[data-color=Black]:not(.outofstock)').removeClass('disabled');
   $(document).on('click', '#selectcolor a', function(e) {
     $('#selectcolor a').removeClass('selected-border');
     $('.cup-size-container:not(.outofstock)').addClass('disabled').removeClass('selected-outline');
     $('.cup-size-container:not(.outofstock)').find('select').attr('name', 'disabled');
     var selectcolor = $(this).attr('data-color');
     $('.cup-size-container[data-color='+selectcolor+']:not(.outofstock)').removeClass('disabled');
     $(this).addClass('selected-border');
     return false;
   });
   $(document).on('click', '.straps-size-container:not(.disabled)', function(e) {
     var strapssizecon = $(this);
     strapssizecon.toggleClass('selected-outline');
     strapssizecon.find('select').attr('name', function(index, attr){ return attr == 'id' ? 'disabled' : 'id';});
     return false;
   });
   $(document).on('click', '.backs-size-container:not(.disabled)', function(e) {
     var backssizecon = $(this);
     backssizecon.toggleClass('selected-outline');
     backssizecon.find('select').attr('name', function(index, attr){ return attr == 'id' ? 'disabled' : 'id';});
     return false;
   });
   var length = $("select[name=id]").length;
   $(document).on('click', '#setp-addtocart', function(e) {
    e.preventDefault();
    var productlist = new Array();
    $('.selected-outline select[name=id]').each(function (index, element) {
       var id = $(this).find('option:selected').val();
       productlist.push({ variant_id: id || 0 });
       console.log(id)
    });
    function ajaxProduct(id){
      if (productlist.length) {
        var request = productlist.shift();
        var tempId= request.variant_id;
        var params = {
          type: 'POST',
          url: '/cart/add.js',
          data: 'quantity=1&id='+tempId,
          dataType: 'json',
          success: function(data) { ajaxProduct(); console.log(tempId) },
          error: function() { ajaxProduct(); }
        };
        $.ajax(params);
      }
      else {
        //document.location.href = '/cart';
        $.getJSON("/cart.js", function(cart) {
          refreshCart(cart);
          window.setTimeout(function(){ $.fancybox.close(); $('.cart-button').click(); }, 500);
        });
      }
    };
    ajaxProduct();
    
    function refreshCart(cart) {
      $cartBtn = $(".icon-cart");
      if ($cartBtn && $cartBtn.length) {
        var value = $cartBtn.text() || '0';
        var cart_items_html = "";
        var $cart = $("#cart ul");
        $cartBtn.text(value.replace(/[0-9]+/,cart.item_count));
        $cart.find('li:not(:first)').remove();
        if (cart.item_count == 0) {
          $cart.append('<li class="empty_cart">{{ 'layout.general.empty_cart' | t }}</li>');
        } else {
          $.each(cart.items, function(index, item) {
          cart_items_html += '<li class="cart_item">' +
            '<p class="mm-counter">' +
              '<span class="ss-icon minus">&#x002D;</span>' +
              '<input type="number" min="0" class="quantity" name="updates[]" id="updates_' + item.id + '" value="' + item.quantity +'" />' +
              '<span class="ss-icon plus">&#x002B;</span>' +
            '</p>' +
            '<a href="' + item.url +'">';
          if (item.image) {
            cart_items_html += '<div class="cart_image">' +
                '<img src="' + item.image.replace(/(\.[^.]*)$/, "_medium$1").replace('http:', '') + '" alt="' + htmlEncode(item.title) + '" />' +
              '</div>';
          }
          cart_items_html += '<div class="item_title">' + item.title +'</div>' +
              '<strong class="price">' + Shopify.formatMoney(item.price, $cart.data('money-format')) + '</strong>' +
            '</a>' +
          '</li>';
          });
          
          cart_items_html += '<li class="mm-label">' +
            '<p class="mm-counter">' + Shopify.formatMoney(cart.total_price, $cart.data('money-format')) + '</p>' +
            '<a href="/cart">' +
              '<strong>{{ 'layout.general.subtotal' | t }}</strong>' +
            '</a>' +
          '</li>' +
          '<li class="mm-subtitle clearfix">';

          {% if settings.display_special_instructions %} 
            var cart_note = cart.note ? cart.note : '';
            cart_items_html += '<textarea id="note" name="note" rows="2" placeholder={{ 'layout.general.cart_note' | t | json }} class="clearfix">'+cart_note+'</textarea>';
          {% endif %}

          {% if settings.display_tos_checkbox %}
            cart_items_html += '<p class="tos"><input type="checkbox" class="tos_agree" id="sliding_agree" required /><label class="tos_label" for="sliding_agree"> ' +
              {{ 'cart.general.agree_to_terms_html' | t | json }} +
            '</label></p>';
          {% endif %}

          cart_items_html += '<input type="submit" class="action_button right" value={{ 'layout.general.checkout' | t | json }} />' +
            '<a href="javascript:void(0)" class="action_button edit_cart right">{{ 'layout.general.continue_shopping' | t }}</a>' +
          '</li>';
          
          $cart.append(cart_items_html);
          $cart.find('li.cart_item:last').addClass('last_cart_item');
          
          {% if settings.show_multiple_currencies %}
          if($('.currencies:visible option:selected').val()) {
            Currency.convertAll($cart.data('shop-currency'), $('.currencies:visible option:selected').val()); 
          }
          {% endif %}
        }
      }
    }
    
    $('.section').removeClass('activate').hide();
    $(this).parents('.section').next('.section').addClass('activate').show();
   });
 });
</script>
