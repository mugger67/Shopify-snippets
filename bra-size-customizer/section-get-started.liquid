        {% assign variantcount = 1 %}
        {% paginate collections['cups'].products by 1000 %}
        {% for product in collections['cups'].products %}
        <div class="one-third column cup-size-container{% for product_option in product.options_with_values %}{% if product_option.name == 'Color' %}{% for value in product_option.values %}{% assign selectcolor = value | remove: 'Manhattan ' | remove: 'Abu Dhabi '%}{% if selectcolor != 'Black' %} disabled{% endif %}{% endfor %}{% endif %}{% endfor %}" data-color="{% for product_option in product.options_with_values %}{% if product_option.name == 'Color' %}{% for value in product_option.values %}{{value | remove: 'Manhattan ' | remove: 'Abu Dhabi '}}{% endfor %}{% endif %}{% endfor %}">
          <a href="#" class=""><img class="cup-size" src="{{product.featured_image | img_url: '700x700'}}" alt="{{product.title | escape}}"></a>
          <div class="stock" style="display: none;">Out of Stock</div>
          <div class="overlay">
            <div class="text">
              <span class="product-title">{{product.title}}</span>
              <span class="product-price">{{product.price | money}}</span>
              <select name="disabled" class="variant-cup-size" id="variant-{% increment variantcount %}" style="display:none;">{% for variant in product.variants %}
                <option{% if variant == product.selected_or_first_available_variant %} selected="selected"{% endif %} data-stock="{% if variant.available %}true{% else %}false{% endif %}" value="{{variant.id}}" data-cupsize="{{variant.title | split: ' / ' | first}}">{{variant.title}}{% if variant.available %}{% else %} - {{ 'products.product.sold_out' | t }}{% endif %}</option>{% endfor %}
