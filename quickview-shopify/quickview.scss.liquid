#quick-view {
  display: none;
  @include flex-wrap(wrap);
  @include justify-content(center);
  position: fixed;
  z-index: 999;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  overflow: auto;
  background-color: #000;
  background-color: rgba(0, 0, 0, 0.7);
  padding: 5% 10%;
  @media screen and (max-width: 990px) {
    padding: 0.5rem;
