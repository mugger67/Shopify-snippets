  &.qv-shown {
  	@media screen and (min-width: 749px) {
    	@include display-flexbox();
    }
  }
  #qv-images-container {
    width: 40%;
    background: #ffffff;
    padding: 2%;
    img {
      height: 100%;
      object-fit: cover;
    }
    @media screen and (max-width: 1350px) {
      width: 60%;
    }
  }
  #qv-content-container {
    position: relative;
    width: 35%;
    background: #ffffff;
    padding: 2%;
    border-top-right-radius: 1em;
    @media screen and (max-width: 1350px) {
